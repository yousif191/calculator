num1=int(input("Enter first number"))

num2=int(input("Enter second number"))

oper=input('''choce * / - +''')  

if oper== '+':
    print (num1,oper,num2,'=',(num1+num2))

elif oper == '-': 
    print (num1,oper,num2,'=',(num1-num2))

elif oper == '/': 
    
    if num2 == 0 :
          print('error')
    else:
          print (num1,oper,num2,'=',(num1/num2))    
    
elif oper =='*':
    print (num1,oper,num2,'=',(num1* num2))    
else:
    print(' error')
